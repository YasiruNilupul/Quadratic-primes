n=2
def check_prime(number):
  score=0
  for i in range(1,number+1):
    if number%i ==0:
      score+=1
  if score==2:
       return "yes"  
  else:
     return 'no' 
   
 

eq_value=[]
for a in range(1000):
    for b in range(1001):
      for x in range(0,n):
       eq =x**2+a*x+b
       eq_value.append(eq)
       
for i in eq_value:
   
     
  check_prime(i)
#print(k)
print(eq_value)
